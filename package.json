{
  "name": "create-radix-tanstack-app",
  "version": "0.0.1",
  "module": "index.ts",
  "type": "module",
  "bin": {
    "create-radix-tanstack-app": "./cli.ts"
  },
  "scripts": {
    "dev": "bun --env-file=.env run vinxi dev --port 4000",
    "build": "tsc && vite build",
    "build-no-errors": "tsc ; vite build",
    "start": "vinxi start",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "tailwindcss": "npx tailwindcss -i ./app/styles/globals.css -o ./public/output.css --watch"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@fontsource/fira-code": "^5.1.1",
    "@fontsource/inter": "^5.1.1",
    "@fontsource/poppins": "^5.1.1",
    "@fontsource/roboto": "^5.1.1",
    "@heroicons/react": "^2.2.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-toast": "^1.2.5",
    "@radix-ui/themes": "^3.1.6",
    "@react-email/components": "^0.0.32",
    "@tailwindcss/postcss": "^4.0.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.0",
    "@tanstack/react-query": "^5.64.2",
    "@tanstack/react-query-devtools": "^5.64.2",
    "@tanstack/react-router": "^1.97.14",
    "@tanstack/router-devtools": "^1.97.14",
    "@tanstack/start": "^1.97.14",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "clsx": "^2.1.1",
    "framer-motion": "^12.0.1",
    "lucide-react": "^0.473.0",
    "next-themes": "^0.4.4",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.0",
    "tailwind-merge": "^2.6.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
